                          <tr>
                            <th>Professional</th>
                            <th>Property </th>
                            <th>Task </th>
                            <th>Notes</th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <%if(activePropertyProposalArray.length>0){%>
                              <%activePropertyProposalArray.forEach(function(row,i){%>
                                <td>
                                  <label class="check-permission"><img src="../images/recent-user-1.png">
                                    <%=row.sp_name%>, <span>
                                        <%=row.sp_profession%>
                                      </span>
                                  </label>
                                </td>
                                <td>
                                  <%=row.ps_property_name%>
                                </td>
                                <td>Task Name</td>
                                <td>
                                  <%=row.notes%>
                                </td>
                                <td><a href="/upload/<%=row.filename%>"><u>View</u></a></td>
                                <!-- <td> <a href="#"><u>Accept</u></a></td> -->
                                <!-- <td><a href="#"><u>Reject</u></a></td>
                                <td><a href="#"><u>Remove</u></a></td> -->
                          </tr>
                          <%})%>
