                <div class="panel">
                  <ul>

                    <div class="todo-list-table">
                      <table class="table table-responsive">
                        <thead>
                          <tr>
                            <th>Professional</th>
                            <th>Property </th>
                            <th>Task </th>
                            <th>Notes</th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <%if(rejectPropertyProposalArray.length>0){%>
