<%- include('dashboard_header_professional')-%>
  <%- include('edit-profile-professional-model')-%>

    <div class="dashboard-wpr">
      <div class="container-fluid">
        <div class="row content">
          <div class="col-sm-2 sidenav hidden-xs-down">
            <div class="user-login text-center">
              <%- include('sp-profile-image')-%>
                <div class="user-name">
                  <h6>
                    <%= session.name%>,<span><i>
                          <%= session.role %>
                        </i>
                      </span>
                  </h6>

                  <a href="#"><i>
                      <%= session.email%>
                        <%- include('edit-profile-professional-icon')-%>
                    </i></a>
                </div>
            </div>
            <%- include('sidebar-professional')-%>
          </div>
          <div class="col-sm-12 col-lg-10">
            <div class="row complaints-sec">
              <div class="col-sm-12 col-lg-6">
                <div class="complaints">
                  <h2>To do list</h2>

                </div>
              </div>
              


            </div>
            <div class="todos-inner">
              <div class="col-sm-12 col-lg-12">
                <form action="/service-provider/professionals-to-do-list" id="pstodoFormtask">
                  <div class="row">
                    <div class="col-sm-12 col-lg-4">
                    <select name="propertyDropDown" class=" form-control selectBox5" id="propertyDropDown"
                      style="height: 48px;">
                      <option value="">Select Property</option>
                      <%if(propertyObj.length!=0){%>
                        <%propertyObj.forEach(function(row,i){%>
                          <option <%if(row._id==property_id){%>selected <%} %> value="<%=row._id%>">
                            <%=row.ps_property_name%>
                          </option>
                          <%});%>
                            <%}else{%>
                              <p>please add property</p>
                              <%}%>
                    </select>
                </div>
                <div class="col-sm-12 col-lg-4">
                <select name="phaseDropDown" class=" form-control selectBox5" id="phaseDropDown"
                  style="height: 48px;"  onchange="return myphaseTaskListToDo()">
                   <option value="">Select Phase</option>
                   <%if(propertyObj.length!=0){%>
                    <%propertyObj[0].ps_phase_array.forEach(function(r,i){%>
                    <option <%if(r.phase_name==phaseName){%>selected <%} %> value="<%=r.phase_name%>"><%=r.phase_name%></option>
                    <%});%>
                    <%}%>
                </select>
                </div>
              </div>
              </form>
              </div>
              <p style="    font-family: 'Abhaya Libre', serif;color: #005377;font-size: 24px;margin-left: 10px; margin-top: 20px;"
                id="propertyName">
                <%if(phaseName){%>
                  <%=phaseName%>
                  <%}else{%>
                    Please Select Property
                    <%}%>
              </p>
              <div class="todo-list-table ">
                <div class="phaselistDiv">
                  <p></p>
                  <%if(taskObject.length>0){%>
                    <table class="table table-phase-fix-2">
                    <thead>
                      <tr>
                        <th>Task</th>
                        <th>Date</th>
                        <th>Status</th>
                        <th>Take Action</th>
                      </tr>
                    </thead>
                    <%if(taskObject){%>
                    <%taskObject.forEach(function(row,i){%>
                    <tr>
                      <td> 
                        <label class="check-todos">
                          <p><%=row.ppts_task_name%></p>
                          
                          <%if(row.iscompleteStatus==true){%>
                            <input type="checkbox" checked disabled>
  
                            <%}else{%>
                              <input type="checkbox">
                              <%}%>
                          <span class="checkmark"></span></label>
                        </td>
                          <td><%=moment(row.ppts_created_at).format('DD-MM-YY')%></td>
                          <td>
                            <%if(row.iscompleteStatus==true){%>
                              <a class="btn btn-complete">Complete</a>
                              <%}else if(row.ispendingStatus==true ){%>
                                <a class="btn btn-pending">Pending</a>
                                
                                <%}else if(row.iscompleteStatus==false && row.ispendingStatus==false){%>
                                  <a class="btn btn-progress">In Progress</a>
                                  <%}%>
                          </td>
                          <td>
                            <a class="take-action" href="/service-provider/myproperties-detail-phase<%=row.ppts_phase_flag%>?id=<%=row.ppts_property_id%>&phase=<%=row.ppts_phase_name%>&step=<%=row.ppts_phase_flag%>" id="">Take Action</a>
                            
                            
                            
                          </td>
                    </tr>
                    <%})%>
                    <%}%>
                    <tbody>
  
                  </tbody>
                  </table></div>
                  <%}%>
                </div>


              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
