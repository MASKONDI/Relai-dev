<%- include('dashboard_header_professional')-%>
  <%- include('edit-profile-professional-model')-%>

    <div class="dashboard-wpr">
      <div class="container-fluid">
        <div class="row content">
          <div class="col-sm-2 sidenav hidden-xs-down">
            <div class="user-login text-center">
              <div class="user-profile">
                <img src="../images/user-3.png" alt="" title="">
              </div>
              <div class="user-name">
                <h6>
                  <%= session.name%>,<span><i>
                        <%= session.role %>
                      </i>
                    </span>
                </h6>

                <a href="#"><i>
                    <%= session.email%>
                      <%- include('edit-profile-professional-icon')-%>
                  </i></a>
              </div>
            </div>
            <%- include('sidebar-professional')-%>
          </div>
          <div class="col-sm-12 col-lg-10">
            <div class="dashboard-inner">
              <div class="row back-to-home">
                <div class="breadcumb">
                  <ul>
                    <li><a href="/service-provider/property"><i class="fa fa-long-arrow-left" aria-hidden="true"></i>
                        Back to All
                        Properties</a></li>
                  </ul>
                </div>
              </div>
              <div class="docs-files-section">
                <div class="row">
                  <div class="col-sm-12 col-lg-12">
                    <div class="apartment-slider">
                      <div class="row">
                        <div class="col-sm-12 col-lg-5">
                          <div id="demo" class="carousel slide" data-ride="carousel">
                            <!-- The slideshow -->
                            <div class="carousel-inner">
                              <% var c=0; propertyImage.forEach(function(rows){%>
                                <%if(c==0){%>
                                  <div class="carousel-item-fix carousel-item active">
                                    <img src="../propimg/<%=rows.pps_property_image_name%>" alt="">
                                  </div>
                                  <%}else{%>
                                    <div class="carousel-item-fix carousel-item ">
                                      <img src="../propimg/<%=rows.pps_property_image_name%>" alt="">
                                    </div>
                                    <%}%>
                                      <% c++;});%>
                            </div>
                            <!-- Left and right controls -->
                            <a class="carousel-control-prev" href="#demo" data-slide="prev">
                              <i class="fa fa-angle-left" aria-hidden="true"></i>
                            </a>
                            <a class="carousel-control-next" href="#demo" data-slide="next">
                              <i class="fa fa-angle-right" aria-hidden="true"></i>
                            </a>
                          </div>
                        </div>
                        <div class="col-sm-12 col-lg-7">
                          <div class="apartment-details">
                            <div class="clearfix">
                              <div class="float-left">
                                <h3 class="apartment-name"><a href="#">
                                    <%=propertyDetailData[0].ps_property_name%><span>(
                                        <%=propertyDetailData[0].ps_unique_code%> )
                                      </span>
                                  </a></h3>
                                <div class="row">
                                  <div class="col-sm-6 col-lg-6">
                                    <div class="location-part"><a href="#"><img src="../images/location.png">
                                        <%=propertyDetailData[0].ps_property_address%>,
                                          <%=propertyDetailData[0].ps_property_country_id%>
                                      </a></div>
                                  </div>

                                  <div class="col-sm-6 col-lg-6">

                                    <div class="recent-img img-fix float-right ">
                                      <% if(customerName=='undefined' || customerProfile=='undefined' ){%>
                                        <img src="../images/user-1.png">
                                        <span style="font-size: 13px;">by
                                          N/A</span>
                                        <%} else {%>
                                          <img src="../upload/<%=customerProfile%>"
                                            style="border-radius: 50%; height: 16px;margin-right: 1px;"><span>
                                            by <%=customerName%>
                                          </span>
                                          <%}%>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
                            <div class="apartment-details-full" style="margin-top: 10px;">
                              <div class="row">
                                <div class="col-4 col-sm-4 col-lg-4">
                                  <div class="start-date">
                                    <p>Area<br><b>
                                        <%=propertyDetailData[0].ps_property_area%> sq.ft.
                                      </b></p>
                                  </div>
                                </div>
                                <div class="col-4 col-sm-4 col-lg-4">
                                  <div class="start-date">
                                    <p>Bedrooms<br><b>
                                        <%=propertyDetailData[0].ps_property_bedroom%>
                                      </b></p>
                                  </div>
                                </div>
                                <div class="col-4 col-sm-4 col-lg-4">
                                  <div class="start-date">
                                    <p>Bathrooms<br><b>
                                        <%=propertyDetailData[0].ps_property_bathroom%>
                                      </b></p>
                                  </div>
                                </div>

                              </div>
                              <div class="row">
                                <div class="col-6 col-sm-4 col-lg-4">
                                  <div class="submit-proposal-fix">
                                    <a href="/service-provider/professional-detail-message"
                                      class="btn submit-proposal-btn">Drop a
                                      Message
                                    </a>
                                  </div>
                                </div>
                                <div class="col-6 col-sm-4 col-lg-4 ">
                                  <div class="submit-proposal-fix">
                                    <a href="/service-provider/property-detail-submit-proposal"
                                      class="btn submit-proposal-btn" data-dismiss="modal">Submit a
                                      Proposal</a>
                                  </div>

                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>


              <div class="professional-files-section">
                <div class="about-section">
                  <h6> <b>About the property</b></h6>
                  <p>
                    <%=propertyDetailData[0].ps_additional_note%>
                  </p>
                </div>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <%-include('professional-footer')-%>
      </body>

      </html>