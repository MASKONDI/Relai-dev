<!-- The Modal -->
<div class="modal permission-modal fade " id="changePasswordModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <form class="form" action="/api/professional-change-password" method="POST" id="changePasswordForm2">
        <!-- Modal Header -->
        <div class="modal-header text-center">
          <h2 class="modal-title w-100">Change Password</h2>
        </div>

        <!-- Modal body -->
        <div class="modal-body">

          <input type="hidden" name="customer_id" value="<%=session.user_id%>">
