              <input type="password" class="form-control form-control-fix2" id="confirmPassword"
                placeholder="confirm Password" name="confirmPassword" value="" data-rule-required="true"
                data-msg-required="confirm password">
            </div>
          </div>

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-cancel" data-dismiss="modal">Cancel</button>
          <button name="submit" type="submit" class="btn btn-cancel">Update</button>
        </div>

      </form>
    </div>
  </div>
</div>


