      var ca=9
      function addmoreBtn(){
        var domElement = $('<div class="kyc-box-portfolio"> <div class=kyc-section-portfolio> <img src="images/upload-icon.png" class="avtar" id="port_addmoreAvtar' + ca + '"> </div> <div class="driving-liences-portfolio"> <p class="port_Picture_' + ca + '" style="white-space: nowrap;overflow: hidden;text-overflow: ellipsis;">Picture/Video </p> </div> <div class="upload-portfolio"> <a href="javascript:void(0)">Upload</a> </div> <span> <div class="custom-file-input"> <input type="file" name="portfolio-docs" style="cursor: pointer;" class="portfolio-docs"onchange="port_previewFile1(this,' + ca + ');" id="image" value="" required> <input type="button"> </div> </span> </div>');
        $('.one').before(domElement);
        ca++
      }
      function update_port_previewFile(input,img_id){
       
        Swal.fire({
        title: 'Are you sure want to Upload Document?',
        showCancelButton: true,
        confirmButtonText: `Yes`,
        confirmButtonColor: '#005377',
      }).then((result) => {
        if (result.isConfirmed) {
          var data = new FormData();
          data.append('portfolio-docs', input.files[0]);
          data.append('img_id', img_id);
          $('.loader-box').attr('style', 'display:flex !important');
          $.ajax({
            type: "POST",
            data: data,
            url: "/update_portPolio",
            cache: false,
            contentType: false,
           processData: false,
            success: function (data) {
              $('.loader-box').attr('style', 'display:none !important');
              console.log(data);
             
              if (data.status == true) {
                Swal.fire({
                  icon: 'success',
                  title: data.message,
                  showConfirmButton: false,
                  confirmButtonColor: '#005377',
                  timer: 3000
                }).then((result)=>{
                  //location.reload(true)
                  window.location.href = '/kyc-professional'
                })

              } else {
                Swal.fire({
                  icon: 'error',
                  title: data.message,
                  showConfirmButton: false,
                  confirmButtonColor: '#005377',
                  timer: 3000
                }).then((result)=>{
                  location.reload(true)
                })
               
            ;
                
              }
            }
          });
        }
      })
      }
      
