                        $('.property_longitude').val(data.propertyData[0].address.geolocation.longitude);
                      }
                      $('.p_address').val(data.propertyData[0].address.buildingNumber + ' ' + data.propertyData[0].address.line1 + ' ' + data.propertyData[0].address.line2 + ' ' + data.propertyData[0].address.line2 + ' ' + data.propertyData[0].address.line3 + ' ' + data.propertyData[0].address.line4);
                      $('#ps_existing_property option[value="Residential"]').attr("selected", "selected");
                      $('#bedroom option[value="' + data.propertyData[0].bedrooms + '"]').attr("selected", "selected");
                      $('#bedroom2 option[value="' + data.propertyData[0].bathrooms + '"]').attr("selected", "selected");
                      $.each(data.offerData, function (i, offerdata) {
                        if(offerdata.status == 'accepted'){
                          let currencyFormat='';
                          if( offerdata.currency == 'GBP'){
                              currencyFormat = '£';
                          }
                          if(offerdata.currency == 'USD'){
                              currencyFormat = '$';
                          }
                            $('.offerPrice').val(currencyFormat+' '+offerdata.amount);
                            $('.offerStatus').val(offerdata.status);
                        }
                      })
                    }
                  })
                }
              });
            }
          })
        });
      </script>